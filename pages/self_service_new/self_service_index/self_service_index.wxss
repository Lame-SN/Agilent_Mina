page{ min-height: 100%; background: #fff; }
.container1 {
    padding: 55.2rpx 16.5rpx;
    display: flex;
    flex-direction: column;
    background-color: #fff;
}

.search {
    display: flex;
    flex-direction: row;
    height: 77.2rpx;
    justify-content: center;
    align-items: center;
    box-shadow: 0 0 22rpx 0 rgba(212, 212, 212, 0.5);
    border-radius: 38rpx;
    padding: 0 33rpx;
    width: 100%;
}
.search input {
    flex: 1;
    height: 44rpx;
    font-size: 33rpx;
    border:none;
    outline: none;
    padding-bottom:1px;
    margin:0;
}

.search image {
    height: 38rpx;
    width: 34rpx;
}

.title {
    margin-top: 20rpx;
    padding-left: 8rpx;
}
.title h3 {
    font-weight: bold;
    font-style: normal;
    font-size: 30rpx;
    color: #636363;
}

.menu1{
    font-size: 28rpx;
    color: #c4c4c4;
    transition: all .3s ease;
    padding: 0 5rpx;
}
.menu2-li {
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    align-items: center;
    padding: 20rpx 0 20rpx;
    padding-right: 10rpx;
    border-bottom-color: #797979;
    border-bottom-style: dotted;
    border-bottom-width: 2rpx;
}
.menu2-li text {
    color: #a3a3a3;
}
.menu2-li image {
    height: 21rpx;
    width: 33rpx;
}
.menu2.li image.open {
    transform: rotate(180deg);
}

.menu3-li {
    padding: 20rpx 0 0;
    display: flex;
    align-items: center;
    -webkit-align-items: center;
}
.menu3-li text {
    color: #636363;
    margin-left: 10rpx;
}
.menu3-li .plus {
    height: 20rpx;
    width: 20rpx;
    background-image: url('data:image/png;base64,');
    background-size: 100% 100%;
}
.menu3-li .open {
    height: 8rpx;
    width: 20rpx;
    background-image: url('data:image/png;base64,');
    background-size: 100% 100%;
}
.menu4-li {
    margin-left: 25rpx;
    color: #636363;
    margin-top: 20rpx;
}

.menu4-li text {
    color: #a3a3a3;
}

.menu5-li {
    padding: 20rpx 0 0;
    display: flex;
    align-items: center;
    -webkit-align-items: center;
    margin-left: 30rpx;
}
.menu5-li text {
    color: #636363;
    margin-left: 10rpx;
}
.menu5-li .plus {
    height: 20rpx;
    width: 20rpx;
    background-image: url('data:image/png;base64,');
    background-size: 100% 100%;
}
.menu5-li .open {
    height: 8rpx;
    width: 20rpx;
    background-image: url('data:image/png;base64,');
    background-size: 100% 100%;
}
.menu6-li {
    margin-left: 55rpx;
    color: #636363;
    margin-top: 20rpx;
}

.menu6-li text {
    color: #a3a3a3;
}


.video_shade {
    display: none;
    position: fixed;
    width: 100%;
    height: 100%;
    top: 0;
    left: 0;
    background-color: rgba(0, 0, 0, 0.15);
    z-index: 1000;
}
.video_shade .video_box {
    width: 600rpx;
    height: 400rpx;
    position: fixed;
    left: 190rpx;
    margin-left: -3rem;
    background-color: #000;
    top: 200rpx;
    border-radius: 16rpx;
    border-top-left-radius: 8rpx;
    box-shadow: 0 0 10rpx 5rpx rgba(42,42,42,0.50);
}
.video_shade .video_box .video_title {
    background-color: #3189d2;
    text-align: center;
    height: 75rpx;
    line-height: 2;
    border-top-left-radius: .15rem;
}
.video_shade .video_box .video_title span {
    color: #fff;
    font-size: 28rpx;
}
.video_shade .video_box .video_title .close_btn {
    background-color: #fff;
    height: .6rem;
    width: .6rem;
    border-radius: 50%;
    position: relative;
    top: -1.2rem;
    left: 5.6rem;
}
.video_shade .video_box .video_title .close_btn img {
    width: 100%;
    height: 100%;
}
.video_shade .video_box video {
    width: 100%;
    height: 100%;
    border-bottom-left-radius: .15rem;
    border-bottom-right-radius: .15rem;
}
video::-webkit-media-controls-mute-button,
video::-webkit-media-controls-current-time-display,
video::-webkit-media-controls-time-remaining-display {
    display: none;
}
