.container {
    padding: 55.2rpx 16.5rpx;
    display: flex;
    flex-direction: column;
    background-color: #fff;
}

.search {
    display: flex;
    flex-direction: row;
    height: 77.2rpx;
    justify-content: center;
    align-items: center;
    box-shadow: 0 0 22rpx 0 rgba(212, 212, 212, 0.5);
    border-radius: 38rpx;
    padding: 0 33rpx;
}
.search input {
    flex: 1;
    height: 44rpx;
    font-size: 33rpx;
    border:none;
    outline: none;
    padding-bottom:1px;
    margin:0;
}

.search image {
    height: 38rpx;
    width: 38rpx;
}
