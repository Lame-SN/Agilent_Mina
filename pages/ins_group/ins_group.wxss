@import '../template/newIns';
/* no_group */
.ng{
  position: fixed;
  width: 100%;
  height: 100%;
  background: #fff;
  top: 0;
  left: 0;
  z-index: 999;
  padding: 0 50rpx;
}
.ng_img{
  width: 340rpx;
  height: 260rpx;
  margin: 150rpx auto 50rpx;
}
.ng_title{
  margin-bottom: 50rpx;
}



/* group_list */
.gl_title{ 
  padding: 20rpx 30rpx;
 }

.gl{
  padding: 35rpx 30rpx;
  background: #fff;
}
.gl:not(:last-child){
  border-bottom: 1px solid #e7e7e7;
}
.g_item{
  flex: .5;
}
.g_image{
  width: 38rpx;
  height: 38rpx;
  margin-left: 10rpx;
}

.edit_image{
  width: 40rpx;
  height: 40rpx;
}
.edit_image1{  
  margin-right: 80rpx;
}




.add_new{
  width: 100%;
  background: #fff;
  padding: 30rpx;
  text-align: center;
  border-top: 1px solid #e7e7e7;
  margin-top: 20rpx;
}



.add_group{
  width: 100%;
  padding: 30rpx 0;
  margin: 30rpx 0;
}
.add_image{
  width: 30rpx;
  height: 30rpx;
  margin-right: 15rpx;
}
.add_input{
  padding:10rpx 0;
  border-bottom: 1px solid #e7e7e7;
  margin: 50rpx 0;
  width: 450rpx;
  text-align: center;
}



.choose_state{
  justify-content: space-between;
}
.finishState{
  margin-left: 50rpx!important;
}