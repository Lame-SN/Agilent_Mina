.container{
    display: flex;
    background-color: #f9f9f9;
    padding: 0;
    flex-direction: column;
    justify-content: normal;
}
.top_bg_view{
    width: 200rpx;
    height: 200rpx;
    background-color: #0287d2;
    border-radius: 128rpx;
    position: relative;
    margin-top: 88rpx;
}


.top_bg_view .waring-img{
    width: 15rpx;
    height: 100rpx;
    left: 95rpx;
    top: 51rpx;
    position: absolute;
}

.top_bg_view .correct-img{
    width: 116rpx;
    height: 100rpx;
    left: 51rpx;
    top: 51rpx;
    position: absolute;
}

.content{
    margin-top: 90rpx;
    font-size: 32rpx;
}
.button{
    margin-top: 130rpx;
    width: 80%;
    border: 1rpx solid #7ebfe8;
    padding: 20rpx;
    border-radius: 64rpx;
    font-size: 30rpx;
    text-align: center;
}

.button text{
    color: #7ebfe8;
}


