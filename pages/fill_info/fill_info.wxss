/* pages/fill_info/fill_info.wxss */
.s-list{ padding: 0 40rpx; background: #fff; }
.s-item{ border-bottom: 1px solid #dfdfdf; padding: 0rpx 8rpx; }
.s-item:last-child{ border: none; }
.item-t{ flex: .3; height: 100rpx; line-height: 100rpx; }
.s-item input{ height: 100rpx; }
.item-r{ flex: .7; }
.serialNo{ text-transform: uppercase; }
.cs_con {
    padding: 20rpx 40rpx 120rpx;
}

.cs_con image {
    width: 44rpx;
    height: 39rpx;
    vertical-align: sub;
}

.cs_con > view {
    line-height: 60rpx;
}